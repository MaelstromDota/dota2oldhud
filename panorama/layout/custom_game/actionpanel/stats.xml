            <Image id="group2" class="pagebutton" src="file://{images}/hud/group_button.png" onactivate="PageMove('2')" />
            <Image id="group3" class="pagebutton" src="file://{images}/hud/group_button.png" onactivate="PageMove('3')" />
            <Image id="group4" class="pagebutton" src="file://{images}/hud/group_button.png" onactivate="PageMove('4')" />
            <Image id="group5" class="pagebutton" src="file://{images}/hud/group_button.png" onactivate="PageMove('5')" />
        </Panel>
    </Panel>
</root>